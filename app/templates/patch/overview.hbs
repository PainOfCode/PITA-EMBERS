
    <div class="col-2 patch_bl">
        {{#if (and auth.user.role.managepatch session.isAuthenticated)}}
        <button {{action "gotoNew"}}>Create new Patch-Note</button>
        {{/if}}
    </div>
    <div class="col-8 bg_nav scrollable patch_bl_ls">
      <div>
          {{#each model as |patch|}}
            {{patch-list patch=patch}}
            {{else}}
            <span class="acc" style="margin-left: 43%;">No patches available</span>
          {{/each}}
        {{outlet}}
      </div>
    </div>
